<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:orientation="horizontal"
        android:gravity="center"
        android:background="#fff">

        <ImageView
            android:id="@+id/IV_tab02Return3_2_1"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:background="@drawable/linear_record_historylist"
            android:src="@mipmap/return_arrows"
            android:descendantFocusability="blocksDescendants"
            />

        <TextView
            android:id="@+id/TV_DownloadTitleView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/view_safeCheck1"
            android:textColor="#000"
            android:textSize="22sp"/>
        <TextView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginRight="20dp"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginTop="5dp"
            android:orientation="vertical">
            <LinearLayout
                android:layout_marginTop="5dp"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:clickable="true"
                android:background="@drawable/linear_record_historylist">
                <LinearLayout
                    android:id="@+id/LL_PeopleInformation"
                    android:layout_width="match_parent"
                    android:layout_marginTop="2.5dp"
                    android:layout_height="72dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/IV_SafeitemViewHead"
                        android:layout_width="72dp"
                        android:layout_height="72dp"
                        android:layout_marginLeft="10dp"
                        android:background="@drawable/button_dayborde" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_marginLeft="4.2dp">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="0.2"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="20dp"
                            android:layout_weight="0.95">
                            <TextView
                                android:id="@+id/TV_SafeitemViewUserName"
                                android:layout_width="wrap_content"
                                android:gravity="center"
                                android:textColor="#000"
                                android:textSize="18dp"
                                android:layout_height="match_parent"
                                android:layout_marginLeft="4dp"
                                android:layout_marginRight="8.5dp"/>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="right">

                                <ImageView
                                    android:id="@+id/IV_itemIcon0"
                                    android:layout_width="12dp"
                                    android:layout_height="12dp"
                                    android:layout_marginRight="2dp"
                                    android:layout_gravity="top"/>

                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_marginTop="-4dp"
                            android:layout_width="match_parent"
                            android:layout_height="5dp"
                            android:layout_marginLeft="4dp"
                            android:layout_weight="1"
                            >
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/TV_SafeitemViewDate"
                                android:layout_marginLeft="4dp"
                                android:text="A0415"
                                android:gravity="center|left"
                                android:textSize="14sp"
                                android:layout_weight="1"/>
                        </LinearLayout>


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="0.5"
                            android:id="@+id/textView4" />

                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="5dp"
                android:clickable="true"
                android:background="@drawable/linear_record_historylist">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="@string/view_power"
                    android:gravity="center"
                    android:textSize="18sp"
                    android:id="@+id/textView5" />

                <TextView
                    android:id="@+id/TV_SafePowertView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="0.2"
                    android:text=""

                    android:gravity="center|left"
                    android:textSize="18sp"
                    android:textColor="@color/colorAccent"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="5dp"
                android:clickable="true"
                android:background="@drawable/linear_record_historylist">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="@string/view_tableChair"
                    android:gravity="center"
                    android:textSize="18sp"
                    android:id="@+id/textView6" />

                <TextView
                    android:id="@+id/TV_SafeChairView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="0.2"
                    android:gravity="center|left"
                    android:textSize="18sp"
                    android:textColor="@color/colorAccent"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="5dp"
                android:clickable="true"
                android:background="@drawable/linear_record_historylist">
                <TextView
                    android:layout_marginLeft="8dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="@string/view_computer"
                    android:gravity="center"
                    android:textSize="18sp"
                    android:id="@+id/textView7" />

                <TextView
                    android:layout_marginLeft="5dp"
                    android:id="@+id/TV_SafeComputerView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="0.23"
                    android:text=""
                    android:gravity="center|left"
                    android:textSize="18sp"
                    android:textColor="@color/colorAccent"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="5dp"
                android:clickable="true"
                android:background="@drawable/linear_record_historylist">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="@string/view_keyMap"
                    android:gravity="center"
                    android:textSize="18sp"
                    android:id="@+id/textView10" />

                <TextView
                    android:id="@+id/TV_SafeKeyMapView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="0.20"
                    android:text=""
                    android:gravity="center|left"
                    android:textSize="18sp"
                    android:textColor="@color/colorAccent"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="5dp"
                android:clickable="true"
                android:background="@drawable/linear_record_historylist">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="@string/view_airConditioner"
                    android:gravity="center"
                    android:textSize="18sp"
                    android:id="@+id/asd" />

                <TextView
                    android:id="@+id/TV_SafeairConditionereView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="0.20"
                    android:text=""
                    android:gravity="center|left"
                    android:textSize="18sp"
                    android:textColor="@color/colorAccent"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="5dp"
                android:clickable="true"
                android:background="@drawable/linear_record_historylist">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="@string/view_sanitation"
                    android:gravity="center"
                    android:textSize="18sp"
                    android:id="@+id/asd1" />

                <TextView
                    android:id="@+id/TV_SafesanitationView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="0.20"
                    android:text=""
                    android:gravity="center|left"
                    android:textSize="18sp"
                    android:textColor="@color/colorAccent"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="5dp"
                android:clickable="true"
                android:background="@drawable/linear_record_historylist">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="@string/view_mainSwitch"
                    android:gravity="center"
                    android:textSize="18sp"
                    android:id="@+id/asd3" />

                <TextView
                    android:id="@+id/TV_SafemainSwitchView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="0.20"
                    android:text=""
                    android:gravity="center|left"
                    android:textSize="18sp"
                    android:textColor="@color/colorAccent"
                    />
            </LinearLayout>


        </LinearLayout>

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="0.45dp"
            android:background="#bfbfbf"/>



    </LinearLayout>

</LinearLayout>
